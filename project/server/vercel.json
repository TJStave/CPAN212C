{
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ], 
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js",
            "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                 "key": "Access-Control-Allow-Origin", "value": "*"
            }
        }
    ]
}