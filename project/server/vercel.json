{
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
        }
    ],
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ], 
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}